fi

curl -b ./.cookies -c ./.cookies --data-binary "{ \"module\":\"$1\",\"class\":\"$2\", \"method\":\"$3\", \"params\":$4}" -H "Content-Type: application/json"  http://localhost:8080/rpc
echo


